---
import Layout from "../layouts/Layout.astro";
import App from "../components/app";
import "../styles/global.css";

export function getStaticPaths() {
  return [
    { params: { tab: 'items' } },
    { params: { tab: 'inventory' } },
  ];
}

const { tab } = Astro.params;
---

<Layout>
  <App client:load tab={tab} />
</Layout>
